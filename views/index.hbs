<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="style.css">
  <title>Document</title>
</head>

<body>
  <br>
  <div class="btn-group" role="group" aria-label="Button group with nested dropdown">
    <button type="lg-button" class="btn btn-warning btn-lg"> <a href="/user/register">Register</a> </button>
    <button type="lg button" class="btn btn-warning btn-lg"><a href="/user/login">Login</a> </button>
    <div class="btn-group" role="group">
    </div>
  </div>
  {{#each title}}
  {{!-- <li>{{this.title}}</li> --}}
  <table class="table table-hover">

    <thead>
      <tr>
        <th style="width: 150px;" scope="col">Type</th>
        <th scope="col"> Company Name</th>
      </tr>
    </thead>
    <tbody>
      <tr class="table-active">
        <th scope="row">PVT LTD</th>
        <td> <a href="/company/details/{{this._id}}">{{this.title}}</a> </td>
      </tr>


    </tbody>
  </table>
  {{/each}}





</body>

</html>